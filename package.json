{
  "name": "weather-now",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "ng:update": "ng update @angular/cli @angular/core --allowDirty && npm audit fix",
    "start": "ng serve",
    "start:dist": "npx http-server ./dist/$npm_package_name/",
    "build": "ng build",
    "build:prod": "ng build -c production",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "npm run lint -- --fix=true",
    "lint:sass": "sass-lint -c ./.sass-lint.yml './src/**/*.scss' --verbose --no-exit",
    "lint:sass:fix": "sass-lint-auto-fix",
    "lint:editorconfig": "git ls-files -z | xargs -0 eclint check",
    "lint:editorconfig:fix": "git ls-files -z | xargs -0 eclint fix",
    "e2e": "ng e2e",
    "bundle-analyzer": "npx webpack-bundle-analyzer ./dist/$npm_package_name/stats*.json --mode static --report ./documentation/bundle/index.html",
    "bundle-analyzer:serve": "npx http-server ./documentation/bundle/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.5",
    "@angular/common": "~8.2.5",
    "@angular/compiler": "~8.2.5",
    "@angular/core": "~8.2.5",
    "@angular/forms": "~8.2.5",
    "@angular/platform-browser": "~8.2.5",
    "@angular/platform-browser-dynamic": "~8.2.5",
    "@angular/router": "~8.2.5",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.4",
    "@angular/cli": "~8.3.4",
    "@angular/compiler-cli": "~8.2.5",
    "@angular/language-service": "~8.2.5",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "eclint": "^2.8.1",
    "husky": "^4.2.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "lint-staged": "^10.0.7",
    "protractor": "~5.4.0",
    "sass-lint": "^1.13.1",
    "sass-lint-auto-fix": "^0.21.0",
    "ts-node": "~7.0.0",
    "tslint": "^5.15.0",
    "typescript": "~3.5.3"
  }
